root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_size = 2
indent_style = space
tab_width = 4
trim_trailing_whitespace = true

[/.gitmodules]
indent_style = tab

[*.md]
indent_size = unset

[*.ini]
end_of_line = crlf

[*.c]
end_of_line = crlf

[*.cpp]
end_of_line = crlf

[*.h]
end_of_line = crlf

[*.cs]
end_of_line = crlf

[*.uproject]
end_of_line = crlf
trim_trailing_whitespace = unset

[*.usf]
end_of_line = crlf

[*.{a,dll,dylib,lib,obj,png,pdb,so,uasset,umap,vrm}]
charset = unset
end_of_line = unset
insert_final_newline = unset
indent_size = unset
indent_style = unset
tab_width = unset
trim_trailing_whitespace = unset
